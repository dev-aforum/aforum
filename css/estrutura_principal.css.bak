@font-face {
 font-family: Caviar;
 src: url("./fontes/Gotham-Book.otf") /* EOT file for IE */
}

@font-face {
 font-family: Caviar;
 src: url("./fontes/Gotham-Book.ttf") /* TTF file for CSS3 browsers */
}

* {
	font-family: "Caviar";
}

img {
	border: 0px;
}

body {
	padding: 0px;
	margin: 0px;
	background: #fff;
}

body#index {
	position: absolute;
	height: 100%;
	width: 100%;
	padding: 0;
	margin: 0;
	background-color: #000;
	overflow: hidden;
}

.clear {
	clear: both;
}

/* LOGOS */
#background {
	position: absolute;
	z-index: 8;
	overflow: hidden;
	width: 110%;
	height: 110%;
	left: -5.85%;
	top: -3.35%;
}

#background img {
	width: 110%;
	left: -5%;
	opacity: 0.5;
    filter: alpha(opacity=50);
}

#over {
	display: none;
}

@media screen and (max-width: 800px){
	#background img {
		opacity: 0.8;
		filter: alpha(opacity=80);
		width: auto !important;
		min-width: 100%;
		min-height: 100%;
	}
	#over {
		display: block;
		position: absolute;
		width: 100%;
		height: 100% !important;
		z-index: 0;
		top: 0;
		left: 0;
		background-color: #000;
		filter: alpha(opacity=30);
		opacity: 0.3;
		z-index: 32;
	}
}

#menu {
	left: 0;
	top: 0;
	width: 100%;
	text-align: center;
}

#logo-menu {
	margin-top: 2%;
	margin-bottom: 2%;
	float: left;
	margin-left: 2%;
	width: 150px;
}

body#index #logo-menu {
	position: absolute;
	z-index: 32000;
	left: 0%;
	margin-top: 25px;
}

@media screen and (max-width: 800px){
	#logo-menu {
		position: relative !important;
		margin-top: 20px !important;
		margin-bottom: 20px !important;
		width: 150px;
		left: 50% !important;
		margin-left: -75px !important;
	}	
}

